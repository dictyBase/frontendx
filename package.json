{
  "name": "dicty-graphql-schema",
  "version": "1.9.0",
  "description": "GraphQL schema and typings for the dictyBase API.",
  "main": "dist/index.js",
  "typings": "types/index.ts",
  "files": [
    "dist",
    "types",
    "package.json"
  ],
  "private": true,
  "engines": {
    "node": ">= 16.13.1"
  },
  "scripts": {
    "generate": "graphql-codegen",
    "build": "tsc types/index.ts --outDir ./dist --esModuleInterop",
    "compile": "yarn run generate && yarn run build",
    "create-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/graphql-schema.git"
  },
  "author": "Siddhartha Basu <siddhartha-basu@northwestern.edu>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/dictyBase/graphql-schema/issues"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "*.graphql"
        ],
        "parser": "@graphql-eslint/eslint-plugin",
        "plugins": [
          "@graphql-eslint"
        ],
        "extends": "plugin:@graphql-eslint/schema-recommended",
        "parserOptions": {
          "schema": "./src/schema/*.graphql"
        },
        "rules": {
          "prettier/prettier": "error"
        }
      }
    ]
  },
  "homepage": "https://github.com/dictyBase/graphql-schema#readme",
  "devDependencies": {
    "@apollo/client": "^3.7.0",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/fragment-matcher": "^3.3.1",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.4",
    "@graphql-codegen/typescript-msw": "^1.1.4",
    "@graphql-codegen/typescript-operations": "2.5.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.4",
    "@graphql-eslint/eslint-plugin": "^3.12.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@yarnpkg/pnpify": "^3.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.4"
  }
}
