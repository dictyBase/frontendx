    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "config": "*",
    "eslint": "^7.32.0",
    "jest": "^27.4.7",
    "react": "^17.0.2",
    "scripts": "*",
    "ts-jest": "^27.1.3",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.2",
    "@material-ui/core": "^4.12.3",
    "react-slidedown": "^2.4.7"
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  }
