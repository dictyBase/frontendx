{
  "name": "@dictybase/header",
  "version": "0.0.0",
  "main": "./src/index.tsx",
  "sideEffects": false,
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "TIMING=1 eslint src/ --ext .ts --ext .tsx -f unix --cache"
  },
  "devDependencies": {
    "@testing-library/react": "^12.x",
    "@testing-library/react-hooks": "^8.x",
    "@testing-library/jest-dom": "^5.x",
    "@types/react": "^17.x",
    "@types/react-dom": "^17.x",
    "@types/uuid": "^9.0.1",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "vitest": "latest",
    "jsdom": "latest"
  },
  "dependencies": {
    "@dictybase/dicty-image": "*",
    "@dictybase/functional": "*",
    "@fontsource/roboto": "~4.5.2",
    "@material-ui/core": "^4.x",
    "@material-ui/icons": "^4.x",
    "@material-ui/lab": "^4.11.3-deprecations.1",
    "fp-ts": "2.13.1",
    "fp-ts-std": "~0.13.1",
    "react": "17.x",
    "react-dom": "17.x",
    "uuid": "9.x"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "testPathIgnorePatterns": [
      "src/e2e/"
    ],
    "coveragePathIgnorePatterns": [
      "dist/",
      "src/data/",
      "src/types/",
      "src/e2e/",
      "src/styles/"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "moduleNameMapper": {
      "fp-ts-std/(.*)": "fp-ts-std/dist/cjs/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "custom"
    ]
  }
}
