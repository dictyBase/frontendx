{
  "name": "publication",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": "10.17.0"
  },
  "homepage": "/publication",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "eslint": "eslint --ext .tsx,.ts src/",
    "precommit": "lint-staged",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.14",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "13.13.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.4",
    "@types/webpack-env": "^1.15.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-link-persisted-queries": "^0.2.2",
    "connected-react-router": "^6.6.1",
    "date-fns": "^1.30.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux",
    "element-remove": "^1.0.4",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-helmet": "^6.0.0",
    "react-loading-skeleton": "^2.0.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "string.prototype.includes": "^1.0.0",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "25.2.2",
    "@types/react-helmet": "^6.0.0",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chromedriver": "^81.0.0",
    "commitizen": "^4.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "geckodriver": "^1.19.1",
    "husky": "^4.2.5",
    "jest-enzyme": "^7.1.2",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^10.1.7",
    "nightwatch": "^1.3.5",
    "nock": "^12.0.3",
    "prettier": "^2.0.5",
    "react-docgen-typescript": "^1.16.3",
    "react-styleguidist": "^11.0.6",
    "redux-mock-store": "^1.5.4",
    "semantic-release": "^17.0.7",
    "source-map-explorer": "^2.4.2",
    "waait": "^1.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/layout/*.{js,jsx,ts,tsx}",
      "src/common/components/*.{js,jsx,ts,tsx}",
      "src/common/utils/*.{js,jsx,ts,tsx}",
      "src/features/**/*.{js,jsx,ts,tsx}",
      "!src/**/mock*.js",
      "!src/**/types.js",
      "!src/**/*Styles.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "ci": false,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  }
}
