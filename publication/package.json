{
  "name": "publication",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": ">= 14"
  },
  "homepage": "/publication",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "tsc": "tsc",
    "lint": "eslint --ext .tsx,.ts src/",
    "pre-commit": "lint-staged",
    "semantic-release": "semantic-release",
    "styleguide": "FAST_REFRESH=false styleguidist server",
    "styleguide:build": "FAST_REFRESH=false styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.3",
    "date-fns": "^1.30.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.2.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-hooks": "dictyBase/dicty-hooks#2.0.0",
    "element-remove": "^1.0.4",
    "fontsource-roboto": "^3.1.5",
    "graphql": "^15.5.0",
    "jwt-decode": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "string.prototype.includes": "^2.0.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "26.0.22",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.8.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "6",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "^1.22.0",
    "react-styleguidist": "^11.1.6",
    "semantic-release": "^17.4.2",
    "source-map-explorer": "^2.5.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts}",
      "!src/common/constants/*.{ts}",
      "!src/common/graphql/*.{ts}",
      "!src/**/mock*.{ts,tsx}",
      "!src/**/types.{ts,tsx}",
      "!src/**/*Styles.{ts,tsx}"
    ],
    "resetMocks": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  }
}
