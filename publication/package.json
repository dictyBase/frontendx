{
  "name": "publication",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": ">= 14.17.0"
  },
  "homepage": "/publication",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "tsc": "tsc",
    "lint": "eslint --ext .tsx,.ts src/",
    "pre-commit": "lint-staged",
    "semantic-release": "semantic-release",
    "styleguide": "FAST_REFRESH=false styleguidist server",
    "styleguide:build": "FAST_REFRESH=false styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.4.17",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "apollo3-cache-persist": "^0.9.1",
    "date-fns": "^2.25.0",
    "dicty-components-header-footer": "dictybase/dicty-components-header-footer#11.0.0",
    "dicty-components-login": "dictybase/dicty-components-login#4.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-graphql-schema": "dictybase/dicty-graphql-schema#1.1.4",
    "dicty-hooks": "dictybase/dicty-hooks#3.0.0",
    "element-remove": "^1.0.4",
    "fontsource-roboto": "^4.0.0",
    "graphql": "^15.7.2",
    "jwt-decode": "^3.0.0",
    "localforage": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^3.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.2",
    "string.prototype.includes": "^2.0.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@material-ui/styles": "^4.11.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "27.0.2",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "16.11.7",
    "@types/react": "17.0.26",
    "@types/react-dom": "17.0.11",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.0",
    "@types/webpack-env": "^1.16.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "react-docgen-typescript": "^2.1.1",
    "react-styleguidist": "^11.1.6",
    "semantic-release": "^18.0.0",
    "source-map-explorer": "^2.5.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts}",
      "!src/common/constants/*.{ts}",
      "!src/common/graphql/*.{ts}",
      "!src/**/mock*.{ts,tsx}",
      "!src/**/types.{ts,tsx}",
      "!src/**/*Styles.{ts,tsx}"
    ],
    "resetMocks": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  }
}
