{
  "name": "publication",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "homepage": "/publication",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test-coverage": "react-scripts test --env=jsdom-fourteen --coverage",
    "test-coverage-watch": "react-scripts test --env=jsdom-fourteen --coverage --watchAll",
    "eject": "react-scripts eject",
    "tsc": "tsc",
    "lint": "eslint --ext .tsx,.ts src/",
    "pre-commit": "lint-staged",
    "semantic-release": "semantic-release",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.10.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-link-persisted-queries": "^0.2.2",
    "connected-react-router": "^6.6.1",
    "date-fns": "^1.30.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-hooks": "dictyBase/dicty-hooks#1.1.0",
    "element-remove": "^1.0.4",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jwt-decode": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "string.prototype.includes": "^2.0.0",
    "typeface-roboto": "0.0.75",
    "typescript": "4.x"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "26.0.14",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "14.11.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.4",
    "@types/redux-mock-store": "^1.0.2",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.9.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.4.0",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "react-docgen-typescript": "^1.20.5",
    "react-scripts": "^3.4.3",
    "react-styleguidist": "^11.1.0",
    "redux-mock-store": "^1.5.4",
    "semantic-release": "^17.1.2",
    "source-map-explorer": "^2.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts}",
      "!src/common/constants/*.{ts}",
      "!src/common/graphql/*.{ts}",
      "!src/**/mock*.{ts,tsx}",
      "!src/**/types.{ts,tsx}",
      "!src/**/*Styles.{ts,tsx}"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
