{
  "name": "publication",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": ">= 14"
  },
  "homepage": "/publication",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test-coverage": "react-scripts test --env=jsdom-fourteen --coverage",
    "test-coverage-watch": "react-scripts test --env=jsdom-fourteen --coverage --watchAll",
    "eject": "react-scripts eject",
    "tsc": "tsc",
    "lint": "eslint --ext .tsx,.ts src/",
    "pre-commit": "lint-staged",
    "semantic-release": "semantic-release",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "^4.11.2",
    "date-fns": "^1.30.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.2.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-hooks": "dictyBase/dicty-hooks#1.1.1",
    "element-remove": "^1.0.4",
    "fontsource-roboto": "^3.1.5",
    "graphql": "^15.3.0",
    "jwt-decode": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "string.prototype.includes": "^2.0.0",
    "typescript": "4.x"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "26.0.19",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "14.14.16",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "nock": "^13.0.5",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "^1.20.5",
    "react-scripts": "^3.4.3",
    "react-styleguidist": "^11.1.5",
    "semantic-release": "^17.3.0",
    "source-map-explorer": "^2.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts}",
      "!src/common/constants/*.{ts}",
      "!src/common/graphql/*.{ts}",
      "!src/**/mock*.{ts,tsx}",
      "!src/**/types.{ts,tsx}",
      "!src/**/*Styles.{ts,tsx}"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,tsx,ts}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}