{
  "name": "dicty-frontpage",
  "version": "0.0.0",
  "scripts": {
    "e2e:local": "FRONTPAGE_URL=http://localhost:3004 && playwright test",
    "dev": "vite",
    "dev:mock": "vite --host --mode mock",
    "dev:seed": "vite --host --mode seed",
    "test": "vitest run",
    "build": "vite build",
    "serve": "vite preview --port 3000",
