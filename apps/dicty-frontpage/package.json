    "snyk": "^1.1187.0",
    "source-map-explorer": "^2.5.3",
    "tsconfig": "*",
    "vite": "4.x",
    "vitest": "0.33.*",
    "vitest-fetch-mock": "^0.2.2",
    "wait-for-expect": "^3.0.2"
