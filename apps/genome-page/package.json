{
  "name": "genomepage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "homepage": "/gene",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test-coverage": "react-scripts test --env=jsdom-fourteen --coverage",
    "test-coverage-watch": "react-scripts test --env=jsdom-fourteen --coverage --watchAll",
    "cypress:open": "cypress open",
    "cypress:run": "percy exec -- cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js src/",
    "tsc": "tsc",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.1.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-hooks": "dictyBase/dicty-hooks#1.1.0",
    "element-remove": "^1.0.4",
    "font-awesome": "^4.7.0",
    "graphql": "^15.3.0",
    "jwt-decode": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "string.prototype.includes": "^2.0.0",
    "typeface-roboto": "0.0.75",
    "typescript": "^4.0.3",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.2",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^11.0.2",
    "@types/jest": "^26.0.10",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cypress": "^4.12.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testing-library": "^3.9.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "react-docgen-typescript": "^1.20.5",
    "react-styleguidist": "^11.1.0",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.1.1",
    "start-server-and-test": "^1.11.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts,tsx}",
      "!src/common/constants/*.{ts,tsx}",
      "!src/common/graphql/*.{ts,tsx}",
      "!src/common/mocks/*.{ts,tsx}",
      "!src/**/*Config.{ts,tsx,sample.ts}",
      "!src/common/utils/icons.ts",
      "!src/app/styleguide/*.{ts,tsx}"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
