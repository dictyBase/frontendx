{
  "name": "genomepage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "homepage": "/gene",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "percy exec -- cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js src/",
    "tsc": "tsc",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.2.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-hooks": "dictyBase/dicty-hooks#1.1.1",
    "element-remove": "^1.0.4",
    "font-awesome": "^4.7.0",
    "fontsource-roboto": "^3.1.5",
    "graphql": "^15.5.0",
    "jwt-decode": "^3.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-loading-skeleton": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "string.prototype.includes": "^2.0.0",
    "typescript": "^4.2.3",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.4",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.7",
    "cypress": "^6.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest-dom": "^3.3.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "5",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "^1.21.0",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.4.2",
    "start-server-and-test": "^1.12.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts,tsx}",
      "!src/common/constants/*.{ts,tsx}",
      "!src/common/graphql/*.{ts,tsx}",
      "!src/common/mocks/*.{ts,tsx}",
      "!src/**/*Config.{ts,tsx,sample.ts}",
      "!src/common/utils/icons.ts",
      "!src/app/styleguide/*.{ts,tsx}"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  }
}
