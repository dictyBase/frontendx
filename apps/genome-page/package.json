{
  "name": "genomepage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "homepage": "/gene",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:mock": "REACT_APP_MOCK_SERVER=on react-scripts start",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "percy exec -- cypress run",
    "cypress:ci": "REACT_APP_MOCK_SERVER=on start-server-and-test start http://localhost:3000 cypress:run",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js src/",
    "tsc": "tsc",
    "styleguide": "FAST_REFRESH=false styleguidist server",
    "styleguide:build": "FAST_REFRESH=false styleguidist build",
    "semantic-release": "semantic-release",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "apollo3-cache-persist": "^0.13.0",
    "dicty-components-header-footer": "dictybase/dicty-components-header-footer#11.0.0",
    "dicty-components-login": "dictybase/dicty-components-login#4.1.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-graphql-schema": "dictybase/dicty-graphql-schema#1.6.0",
    "dicty-hooks": "dictybase/dicty-hooks#3.0.0",
    "element-remove": "^1.0.4",
    "font-awesome": "^4.7.0",
    "fontsource-roboto": "^4.0.0",
    "graphql": "^15.7.2",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.0.1",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.2",
    "string.prototype.includes": "^2.0.0",
    "typescript": "^4.5.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@material-ui/styles": "^4.11.4",
    "@percy/cli": "^1.0.0-beta.71",
    "@percy/cypress": "3.1.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "cypress": "^9.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "7",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "react-docgen-typescript": "^2.2.1",
    "react-styleguidist": "^11.1.7",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^18.0.1",
    "start-server-and-test": "^1.14.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/common/@types/*.{ts,tsx}",
      "!src/common/constants/*.{ts,tsx}",
      "!src/common/graphql/*.{ts,tsx}",
      "!src/common/mocks/*.{ts,tsx}",
      "!src/mocks/*.{js,jsx,ts,tsx}",
      "!src/**/*Config.{ts,tsx,sample.ts}",
      "!src/common/utils/icons.ts",
      "!src/app/styleguide/*.{ts,tsx}",
      "!src/index.tsx"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended",
      "prettier",
      "plugin:jest-dom/recommended"
    ],
    "plugins": [
      "prettier",
      "jest-dom"
    ],
    "rules": {
      "import/exports-last": [
        "error"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
