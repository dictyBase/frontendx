{
  "name": "genomepage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "homepage": "/gene",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "precommit": "lint-staged",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "local-ci-eric": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p ericdev",
    "local-ci-sidd": "IMAGE_TAG=$(git symbolic-ref -q HEAD | sed -e 's|^refs/heads/||' -e 's~/~-~')-$(git rev-parse --short HEAD) skaffold run -p devsidd",
    "local-ci-staging": "IMAGE_TAG=staging-$(git rev-parse --short HEAD) skaffold run"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "@material-ui/icons": "^2.0.0",
    "ProtVista": "^2.0.11",
    "connected-react-router": "^4.4.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#v7.0.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#v1.4.0",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#v0.2.0",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux#v2.3.0",
    "element-remove": "^1.0.3",
    "jwt-decode": "^2.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.1",
    "react-loading-skeleton": "^0.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "string.prototype.includes": "^1.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "ajv": "^6.5.2",
    "commitizen": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "flow-bin": "^0.83.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "react-styleguidist": "^7.3.11",
    "react-test-renderer": "^16.8.6",
    "recast": "^0.13.0",
    "semantic-release": "^15.13.24",
    "sinon": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "release": {
    "noCi": true,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  }
}
