    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8",
    "lint-staged": "^13.1.2",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "react-docgen-typescript": "^2.2.1",
    "react-styleguidist": "^13.1.1",
