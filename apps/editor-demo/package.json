{
  "name": "editor-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3005 --clearScreen false",
    "build": "tsc && vite build",
    "server": "node server.js",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "TIMING=1 eslint src/ --ext .ts --ext .tsx -f unix --cache",
    "lint:out": "eslint src/ --ext .ts --ext .tsx -f json -o ../../eslint/editor-demo.json"
  },
  "dependencies": {
    "@dictybase/json-plugin": "*",
    "@fontsource/fira-code": "^5.0.3",
    "@material-ui/core": "^4.x",
    "@material-ui/icons": "^4.x",
    "editor": "*",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "devDependencies": {
    "@lexical/react": "0.11.x",
    "@types/react": "^17.x",
    "@types/react-dom": "^17.x",
    "@vitejs/plugin-react-swc": "^3.x",
    "body-parser": "^1.20.2",
    "eslint-config-custom": "*",
    "express": "^4.18.2",
    "lexical": "0.11.x",
    "prettier": "^2.x",
    "tsconfig": "*",
    "typescript": "^4.9.x",
    "vite": "4.x",
    "vite-express": "^0.9.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "custom"
    ]
  }
}
