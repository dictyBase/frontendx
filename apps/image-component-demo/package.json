{
  "name": "image-component-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 3004 --clearScreen false",
    "build": "tsc && vite build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
